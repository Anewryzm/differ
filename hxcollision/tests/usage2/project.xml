<?xml version="1.0" encoding="utf-8"?>
<project>

  <app  title="hxcollision usage example 2" 
        file="Main" 
        main="Main" 
        package="com.underscorediscovery.hxcollision" 
        version="1.0.0" 
        company="underscorediscovery" 
  />
   
  <window 
        width="500" 
        height="500"  
        background="#140c1c" 
        fps="60" 
        orientation="landscape" 
        resizable="false" 
  />
 
  <set name="BUILD_DIR" value="Export" />

  <classpath name="Source" />

  <assets path="Assets" rename="assets" include="*" exclude="nme.svg" />  

  <haxelib name="openfl" />
  <haxelib name="hxcollision" />  

  <ndll name="std" />
  <ndll name="regexp" />
  <ndll name="zlib" />
  <ndll name="nme" haxelib="nme" />

  <icon name="Assets/nme.svg" />

</project>